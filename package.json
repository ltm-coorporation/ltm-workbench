{
  "name": "ltm-workbench",
  "version": "1.0.0",
  "description": "Official ltm workbench desktop application",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ltm-workbench --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ltm workbench\"",
    "package-linux": "electron-packager . ltm-workbench --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltm-coorporation/ltm-workbench.git"
  },
  "keywords": [
    "ltm",
    "ltm-coorporation",
    "ltm-workbench"
  ],
  "author": "prashant dawar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ltm-coorporation/ltm-workbench/issues"
  },
  "homepage": "https://github.com/ltm-coorporation/ltm-workbench#readme",
  "devDependencies": {
    "electron": "^3.0.10"
  },
  "dependencies": {
    "electron-reload": "^1.3.0"
  }
}
